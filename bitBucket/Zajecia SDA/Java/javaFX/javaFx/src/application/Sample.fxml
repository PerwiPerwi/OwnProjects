<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="420.0" prefWidth="342.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SampleController">
   <children>
      <Button mnemonicParsing="false" onAction="#showSmth" prefHeight="25.0" prefWidth="200.0" text="NIESPODZIANKA" />
      <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="200.0" text="Button" />
      <TextField fx:id="firstField" onKeyReleased="#showSmth" promptText="Wprowadź imię" />
      <TextField fx:id="secondField" onKeyReleased="#showSmth" promptText="Wprowadź nazwisko" />
      <TextField fx:id="thirdField" promptText="Wprowadź email" />
      <TextField fx:id="fourthField" promptText="Wprowadź pesel" />
      <TextField fx:id="textArea" prefHeight="175.0" prefWidth="342.0" promptText="Wprowadź tekst" />
      <DatePicker fx:id="date" prefHeight="25.0" prefWidth="343.0" />
      <CheckBox mnemonicParsing="false" text="Wyróżnienie ogłoszenia" />
   </children>
</VBox>
